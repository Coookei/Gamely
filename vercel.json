{
  "version": 2,
  "redirects": [
    {
      "source": "/api/",
      "destination": "/api",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://proxy.gamely.tomfox.tech/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}